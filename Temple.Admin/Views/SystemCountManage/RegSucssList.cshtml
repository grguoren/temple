@{
    ViewBag.Title = "無極證道-管理系统-统计管理-成功率统计";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-header position-relative">
    <h1>
        统计管理
        <small>
            <i class="icon-double-angle-right"></i>
            成功率统计
        </small>
    </h1>
</div>

<div class="row-fluid" ms-controller="regsucssList">
    <div class="col-lg-6 col-lg-offset-3">
        <div class="table-responsive">
            <table id="sample-table-1" class="table table-bordered table-hover no-margin-bottom">
                <thead>
                    <tr>
                        <th>芳华云品牌</th>
                        <th>已开通人数</th>
                        <th>註册人数</th>
                        <th>成功率</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ms-if="!ListLoading"><td colspan="4" style="text-align:center;"><i class="icon-spinner icon-spin orange bigger-125"></i></td></tr>
                    <tr ms-if="!ListNoData"><td colspan="4" style="text-align:center;">對不起,沒有數據!</td></tr>
                    <tr ms-repeat="array" ms-visible="ListLoading">
                        <td>{{el.BrandName}}</td>
                        <td>{{el.OpenCount}}</td>
                        <td>{{el.RegCount}}</td>
                        <td>{{el.Success}}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>